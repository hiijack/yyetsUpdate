{
  "manifest_version": 2,

  "name": "One-click Kittens",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "permissions": [
    "storage",
    "http://www.yyets.com/"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
"content_scripts": [{  
         "matches": ["http://www.yyets.com/"],   
         "js": ["catch.js"]
    }]
}
